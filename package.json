{
  "dependencies": {
    "@ghostery/adblocker-electron": "^2.1.1",
    "@xhayper/discord-rpc": "^1.2.0",
    "cross-fetch": "^4.0.0",
    "discord.js-selfbot-v13": "^3.4.5",
    "dotenv": "^16.4.7",
    "electron-forge": "^5.2.4",
    "gyp": "^0.5.0",
    "node-addon-api": "^8.3.0",
    "node-gyp": "^11.0.0",
    "nodemon": "^3.1.7"
  },
  "name": "scloudrpc",
  "version": "1.0.0",
  "main": "main.js",
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-prebuilt-compile": "8.2.0",
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "appId": "com.github.1sheol.soundcloud-discord-rpc",
    "productName": "SoundCloudRPC",
    "mac": {
      "category": "public.app-category.music",
      "target": "dmg",
      "icon": "/icons/logo.png"
    },
    "win": {
      "target": "nsis",
      "icon": "/icons/logo.ico"
    },
    "linux": {
      "category": "Audio;AudioVideo",
      "target": ["snap", "AppImage", "deb", "rpm"],
      "icon": "/icons/logo.png"
    },
    "files": [
      "main.js",
      "preload.js"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app": "node -e \"require('./start.js')\"",
    "winmake": "npx electron-builder build --win", 
    "dev" : "npx electron --trace-warnings ./main.js"
  },
  "keywords": [],
  "author": {
    "name": "1sheol",
    "email": "contact@1sheol.xyz"
  },
  "license": "ISC",
  "description": "",
  "homepage": "https://github.com/Ivy-js/SCloudRPC"
}
